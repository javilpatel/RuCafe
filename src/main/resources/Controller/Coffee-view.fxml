<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="387.0" prefWidth="573.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.CoffeeViewController">
   <children>
      <Text fill="#0606c9" layoutX="223.0" layoutY="48.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Coffee">
         <font>
            <Font name="Sinhala MN Bold" size="38.0" />
         </font>
      </Text>
      <CheckBox fx:id="sweetCream" layoutX="60.0" layoutY="120.0" mnemonicParsing="false" text="Sweet Cream" />
      <CheckBox fx:id="frenchVanilla" layoutX="60.0" layoutY="167.0" mnemonicParsing="false" text="French Vanilla" />
      <CheckBox fx:id="irishCream" layoutX="60.0" layoutY="216.0" mnemonicParsing="false" text="Irish Cream" />
      <CheckBox fx:id="mocha" layoutX="172.0" layoutY="120.0" mnemonicParsing="false" text="Mocha" />
      <CheckBox fx:id="caramel" layoutX="172.0" layoutY="167.0" mnemonicParsing="false" text="Caramel" />
      <ImageView fx:id="coffeeImage" fitHeight="137.0" fitWidth="189.0" layoutX="363.0" layoutY="116.0" pickOnBounds="true" preserveRatio="true" />
      <ComboBox fx:id="coffeeSize" layoutX="275.0" layoutY="116.0" onAction="#sizePicked" prefHeight="26.0" prefWidth="69.0" />
      <ComboBox fx:id="setQuantity" layoutX="275.0" layoutY="163.0" prefHeight="26.0" prefWidth="69.0" />
      <TextField fx:id="subtotal" layoutX="275.0" layoutY="212.0" prefHeight="26.0" prefWidth="69.0" />
      <Button fx:id="addToOrder" layoutX="420.0" layoutY="325.0" mnemonicParsing="false" onAction="#addToOrder" text="add to Order" />
   </children>
</AnchorPane>
